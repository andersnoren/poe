						"bottom": "0px",
						"left": "0px",
						"right": "0px",
						"top": "0px"
					}
				}
			},
			"core/site-tagline": {
				"spacing": {
					"margin": {
						"bottom": "0px",
						"top": "0px"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)"
				}
			},
			"core/site-title": {
				"spacing": {
					"margin": {
						"bottom": "0px",
						"top": "0px"
					}
				},
				"typography": {
					"fontWeight": "var(--wp--custom--typography--font-weight--regular)"
				}
			},
			"core/spacer": {
				"spacing": {
					"margin": {
						"bottom": "0px",
						"top": "0px"
					}
				}
			},
			"core/table": {
