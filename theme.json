					"name": "Small"
				},
				{
					"slug": "normal",
					"size": "clamp(var(--wp--custom--typography--font-sizes--normal--min), var(--wp--custom--typography--font-sizes--normal--pref), var(--wp--custom--typography--font-sizes--normal--max))",
					"name": "Normal"
