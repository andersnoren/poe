		"custom": {
			"spacing": {
				"baseline": "16px",
				"gap": {
					"horizontal": "clamp( calc( 1.5 * var( --wp--custom--spacing--baseline ) ), 2.222vw, calc( 2 * var( --wp--custom--spacing--baseline ) ) )",
					"vertical": "clamp( calc( 1.5 * var( --wp--custom--spacing--baseline ) ), 3.333vw, calc( 3 * var( --wp--custom--spacing--baseline ) ) )"
				},
				"gutter": "clamp( calc( 1.5 * var( --wp--custom--spacing--baseline ) ), 3.333vw, calc( 3 * var( --wp--custom--spacing--baseline ) ) )",
				"inner-container": "var( --wp--custom--spacing--gutter )"
			},
			"typography": {
				"font-sizes": {
					"tiny": { 
						"min": "14px",
						"pref": "1.111vw",
						"max": "16px"
					},
					"small": { 
						"min": "16px",
						"pref": "1.25vw",
						"max": "18px"
					},
					"normal": { 
						"min": "17px",
						"pref": "1.45vw",
						"max": "21px"
					},
					"large": { 
						"min": "21px",
						"pref": "1.666vw",
						"max": "24px"
					},
					"extra-large": { 
						"min": "24px",
						"pref": "2.222vw",
						"max": "32px"
					},
					"huge": { 
						"min": "36px",
						"pref": "4.444vw",
						"max": "64px"
					},
					"gigantic": { 
						"min": "64px",
						"pref": "6.666vw",
						"max": "96px"
					},
					"heading-1": { 
						"min": "35px",
						"pref": "4.444vw",
						"max": "64px"
					},
					"heading-2": { 
						"min": "32px",
						"pref": "3.888vw",
						"max": "56px"
					},
					"heading-3": { 
						"min": "28px",
						"pref": "3.333vw",
						"max": "48px"
					},
					"heading-4": { 
						"min": "24px",
						"pref": "2.777vw",
						"max": "40px"
					},
					"heading-5": { 
						"min": "22px",
						"pref": "2.222vw",
						"max": "32px"
					},
					"heading-6": { 
						"min": "20px",
						"pref": "1.666vw",
						"max": "24px"
					}
				},
				"font-weight": {
					"regular": 400,
					"medium": 500,
					"semibold": 600,
					"bold": 700
				},
				"letter-spacing": {
					"body": "-0.018em",
					"headings": {
						"large": "-0.04em",
						"small": "-0.024em"
					},
					"uppercase": "0"
				},
				"line-height": {
					"body": "calc( 1em + 10px )",
					"heading": 1.25,
					"gigantic": 1.05
				}
			}
		},
		"layout": {
			"contentSize": "640px",
			"wideSize": "1920px"
		},
		"spacing": {
			"blockGap": true,
			"customPadding": true,
			"customMargin": true,
			"units": [ "%", "px", "em", "rem", "vh", "vw" ]
		},
		"typography": {
			"customFontSize": true,
			"customLineHeight": true,
			"fontFamilies": [
				{
					"fontFamily": "Inter, -apple-system, BlinkMacSystemFont, \"Segoe UI\", Roboto, \"Helvetica Neue\", sans-serif",
					"slug": "inter",
					"name": "Inter"
				},
				{
					"fontFamily": "ui-sans-serif, -apple-system, BlinkMacSystemFont, \"Segoe UI\", Roboto, \"Helvetica Neue\", sans-serif",
